{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-over-type-literal": false,
        "member-access": [true, "no-public"],
        "no-console": [true, "log"],
        "quotemark": [true, "single", "jsx-double"],
        "variable-name": [true, "allow-leading-underscore", "allow-pascal-case"]
    },
    "rulesDirectory": []
}